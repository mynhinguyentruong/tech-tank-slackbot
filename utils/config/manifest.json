{
  "display_information": {
    "name": "Nemo",
    "description": "Your friendly neighbourhood fishy",
    "background_color": "#6477b0"
  },
  "features": {
    "app_home": {
      "home_tab_enabled": true,
      "messages_tab_enabled": false,
      "messages_tab_read_only_enabled": true
    },
    "bot_user": {
      "display_name": "Nemo",
      "always_online": true
    },
    "slash_commands": [
      {
        "command": "/random",
        "description": "Selects random member",
        "should_escape": false
      }
    ]
  },
  "oauth_config": {
    "scopes": {
      "bot": [
        "groups:history",
        "im:history",
        "channels:history",
        "chat:write",
        "app_mentions:read",
        "channels:read",
        "im:write",
        "commands",
        "users:read"
      ]
    }
  },
  "settings": {
    "event_subscriptions": {
      "bot_events": [
        "app_home_opened",
        "team_join",
        "message.channels",
        "message.im",
        "message.groups"
      ]
    },
    "interactivity": {
      "is_enabled": true
    },
    "org_deploy_enabled": false,
    "socket_mode_enabled": true,
    "token_rotation_enabled": false
  }
}
